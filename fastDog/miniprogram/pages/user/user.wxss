/* miniprogram/pages/user/user.wxss */
.van-cell-group{
  margin-top: 30rpx;
}
.van-cell{
  margin-top: 3px;
  margin-bottom: 3px;
}
.van-cell__value{
  color: #e6454a;
}
.van-cell__title{
  margin-left: 10px;
  color: #333333;
}
.userinfo{
  position: relative;
  width: 100%;
  height: 200rpx;
}
.border{
  height: 10px;
  background-color: #f5f5f5;
}
.userAvatar{
  margin-top: 25px;
  margin-left: 20px;
  width: 110rpx;
  height: 110rpx;
  border-radius: 110rpx;
  display: inline-block;
}

.nickname{
  position: absolute;
  margin-top: 25px;
  margin-left: 20px;
  display: inline-block;
  height: 50rpx;
  color: #333333;
}
